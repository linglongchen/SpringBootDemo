package com.chunqiu.mrjuly.modules.install.model;

import com.chunqiu.mrjuly.common.persistence.DataEntity;

/**
 * 奖品设置
 */
public class PrizeInfo extends DataEntity<PrizeInfo, Integer> {
    private static final long serialVersionUID = 1L;
    private Integer id;
    private String prize;
    private String prizeName;
    private String prizeImg;
    private Integer prizeType;

    private String name;
    private String gender;
    private Integer hotelCodeId;
    private String room;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public Integer getHotelCodeId() {
        return hotelCodeId;
    }

    public void setHotelCodeId(Integer hotelCodeId) {
        this.hotelCodeId = hotelCodeId;
    }

    public String getRoom() {
        return room;
    }

    public void setRoom(String room) {
        this.room = room;
    }

    @Override
    public Integer getId() {
        return id;
    }

    @Override
    public void setId(Integer id) {
        this.id = id;
    }

    public String getPrize() {
        return prize;
    }

    public void setPrize(String prize) {
        this.prize = prize;
    }

    public String getPrizeName() {
        return prizeName;
    }

    public void setPrizeName(String prizeName) {
        this.prizeName = prizeName;
    }

    public String getPrizeImg() {
        return prizeImg;
    }

    public void setPrizeImg(String prizeImg) {
        this.prizeImg = prizeImg;
    }

    public Integer getPrizeType() {
        return prizeType;
    }

    public void setPrizeType(Integer prizeType) {
        this.prizeType = prizeType;
    }
}
