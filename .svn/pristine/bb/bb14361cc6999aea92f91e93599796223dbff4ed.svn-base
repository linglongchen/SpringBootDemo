package com.chunqiu.mrjuly.modules.shopinfo.model;

import com.chunqiu.mrjuly.common.persistence.DataEntity;
import com.chunqiu.mrjuly.modules.install.model.ServiceMode;

import java.util.List;

/**
 * 商品创建Entity
 * @author wyy
 * @version 2019-03-14
 */
public class ProductCreate extends DataEntity<ProductCreate, Integer> {

	private static final long serialVersionUID = 1L;
	private Integer shopId;  //店铺id
	private String  goodsPic; //商品图片
	private String  goodsInfoPic; //商品详情图片
	private String  goodsName;   //商品名称
	private float  goodsPrice;   //商品价格
	private Integer goodsNum;   //商品数量
	private String  serviceMode;  //服务方式
	private String  goodsVideo; //商品视频
	private Integer stock;   //库存 1.售罄 2.有货
	private Integer status;   //0.未上架 1.已上架
	private String  goodsDescribe;  //商品描述
	private String idList;//存储id的list
	private Integer id;
	private List<String> goodsPicList ;//商品图片list
	private List<String> goodsPicInfoList ;//商品详情图片list


	public String getIdList() {
		return idList;
	}

	public void setIdList(String idList) {
		this.idList = idList;
	}

	@Override
	public Integer getId() {
		return id;
	}

	@Override
	public void setId(Integer id) {
		this.id = id;
	}

	public List<String> getGoodsPicList() {
		return goodsPicList;
	}

	public void setGoodsPicList(List<String> goodsPicList) {
		this.goodsPicList = goodsPicList;
	}

	public List<String> getGoodsPicInfoList() {
		return goodsPicInfoList;
	}

	public void setGoodsPicInfoList(List<String> goodsPicInfoList) {
		this.goodsPicInfoList = goodsPicInfoList;
	}

	public static long getSerialVersionUID() {
		return serialVersionUID;
	}

	public Integer getShopId() {
		return shopId;
	}

	public void setShopId(Integer shopId) {
		this.shopId = shopId;
	}

	public String getGoodsPic() {
		return goodsPic;
	}

	public void setGoodsPic(String goodsPic) {
		this.goodsPic = goodsPic;
	}

	public String getGoodsInfoPic() {
		return goodsInfoPic;
	}

	public void setGoodsInfoPic(String goodsInfoPic) {
		this.goodsInfoPic = goodsInfoPic;
	}

	public String getGoodsName() {
		return goodsName;
	}

	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}

	public float getGoodsPrice() {
		return goodsPrice;
	}

	public void setGoodsPrice(float goodsPrice) {
		this.goodsPrice = goodsPrice;
	}

	public Integer getGoodsNum() {
		return goodsNum;
	}

	public void setGoodsNum(Integer goodsNum) {
		this.goodsNum = goodsNum;
	}

	public String getServiceMode() {
		return serviceMode;
	}

	public void setServiceMode(String serviceMode) {
		this.serviceMode = serviceMode;
	}

	public String getGoodsVideo() {
		return goodsVideo;
	}

	public void setGoodsVideo(String goodsVideo) {
		this.goodsVideo = goodsVideo;
	}

	public Integer getStock() {
		return stock;
	}

	public void setStock(Integer stock) {
		this.stock = stock;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getGoodsDescribe() {
		return goodsDescribe;
	}

	public void setGoodsDescribe(String goodsDescribe) {
		this.goodsDescribe = goodsDescribe;
	}
}
