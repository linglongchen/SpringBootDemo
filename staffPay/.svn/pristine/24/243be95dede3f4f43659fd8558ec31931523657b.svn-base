    <script type="text/javascript" th:inline="javascript">
        var ctx = [[${ctx}]];

        //弹框退出提示框
      function exitBombSow(index){
          layer.confirm('关闭或退出不会对您的任何编辑操作进行保存！',{icon:"3",title:"提示"},function(index1){
              layer.close(index1);
              layer.close(index);
              return true;
          });
        }





        $(function(){

            //使用layui的表单模块
            layui.use(['form','layer'],function () {
                var layer = layui.layer;
                //定义layer默认风格
                layer.config({
                    skin: 'layui-layer-molv'
                });
            });



        });

        //获取字典表内容
        function getDictLabel(data, value, defaultValue){
            for (var i = 0; i < data.length; i++){
                var row = data[i];
                if (row.value == value){
                    return row.label;
                }
            }
            return defaultValue;
        }


        function clearForm(objE){//objE为form表单
            $(objE).find(':input').each(
                function(){
                    switch(this.type){
                        case 'passsword':
                        case 'select-multiple':
                        case 'select-one':
                        case 'text':
                        case 'textarea':
                            $(this).val('');
                            break;
                        case 'checkbox':
                        case 'radio':
                            this.checked = false;
                    }
                }
            );
        }
        /**
         * 将form表单元素的值序列化成对象
         *
         * @example hh.serializeObject($('#formId'))
         * @author wcf
         * @requires jQuery
         * @returns object
         */
        var serializeObject = function(form) {
            var o = {};
            $.each(form.serializeArray(), function(index) {
                if (this['value'] != undefined && this['value'].length > 0) {// 如果表单项的值非空，才进行序列化操作
                    if (o[this['name']]) {
                        o[this['name']] = o[this['name']] + "," + this['value'];
                    } else {
                        o[this['name']] = this['value'];
                    }
                }
            });
            return o;
        };
    </script>

    <head th:include="init_form :: init_form"></head>


    <script th:if="${error} != null">
        $(function(){
