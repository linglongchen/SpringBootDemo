package com.chunqiu.mrjuly.modules.shopinfo.model;


/**
 * 商场列表
 * 
 * @author wy
 *
 */
public class MarketListModel {

	private Long userId;// admin_user对应ID

	private Long marketId;// 商场ID,admin_user对应ID

	private String marketName;// 商场名称

	private Long shopId;// 商场shop_info对应ID

	private Integer bindNum;// 已绑定商家数量

	private Integer hotelNum;// 已入驻酒店数量
	
	private Double balance;//商场余额
	
	private Byte freeze;//冻结状态:0-正常,1-冻结
	
	private Byte starLevel;//星级
	
	private String shopTel;//联系电话
	
	private String shopNumber;//商场编号
	
	private Long hotelId;//酒店ID,查询使用

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public Long getMarketId() {
		return marketId;
	}

	public void setMarketId(Long marketId) {
		this.marketId = marketId;
	}

	public String getMarketName() {
		return marketName;
	}

	public void setMarketName(String marketName) {
		this.marketName = marketName;
	}

	public Long getShopId() {
		return shopId;
	}

	public void setShopId(Long shopId) {
		this.shopId = shopId;
	}

	public Integer getBindNum() {
		return bindNum;
	}

	public void setBindNum(Integer bindNum) {
		this.bindNum = bindNum;
	}

	public Integer getHotelNum() {
		return hotelNum;
	}

	public void setHotelNum(Integer hotelNum) {
		this.hotelNum = hotelNum;
	}

	public Double getBalance() {
		return balance;
	}

	public void setBalance(Double balance) {
		this.balance = balance;
	}

	public Byte getFreeze() {
		return freeze;
	}

	public void setFreeze(Byte freeze) {
		this.freeze = freeze;
	}

	public Byte getStarLevel() {
		return starLevel;
	}

	public void setStarLevel(Byte starLevel) {
		this.starLevel = starLevel;
	}

	public String getShopTel() {
		return shopTel;
	}

	public void setShopTel(String shopTel) {
		this.shopTel = shopTel;
	}

	public String getShopNumber() {
		return shopNumber;
	}

	public void setShopNumber(String shopNumber) {
		this.shopNumber = shopNumber;
	}

	public Long getHotelId() {
		return hotelId;
	}

	public void setHotelId(Long hotelId) {
		this.hotelId = hotelId;
	}

}