package com.chunqiu.mrjuly.modules.shopinfo.model;

public class MarketBindShopInfo {
	private Long shopId;// 商家ID
	private String shopName;// 商家名称
	private Byte freeze;// 状态:0-正常,1-冻结
	private String shopLogo;// 商家LOGO
	private String shopAddress;// 商家地址

	public Long getShopId() {
		return shopId;
	}

	public void setShopId(Long shopId) {
		this.shopId = shopId;
	}

	public String getShopName() {
		return shopName;
	}

	public void setShopName(String shopName) {
		this.shopName = shopName;
	}

	public Byte getFreeze() {
		return freeze;
	}

	public void setFreeze(Byte freeze) {
		this.freeze = freeze;
	}

	public String getShopLogo() {
		return shopLogo;
	}

	public void setShopLogo(String shopLogo) {
		this.shopLogo = shopLogo;
	}

	public String getShopAddress() {
		return shopAddress;
	}

	public void setShopAddress(String shopAddress) {
		this.shopAddress = shopAddress;
	}

}
